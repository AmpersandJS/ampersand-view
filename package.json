{
  "name": "ampersand-view",
  "description": "A smart base view for Backbone apps, to make it easy to bind collections and properties to the DOM.",
  "version": "7.1.5",
  "author": "Henrik Joreteg <henrik@andyet.net>",
  "browser": "./ampersand-view.js",
  "browserify": {
    "transform": [
      "ampersand-version"
    ]
  },
  "bugs": "https://github.com/ampersandjs/ampersand-view/issues",
  "dependencies": {
    "ampersand-collection-view": "^1.0.0",
    "ampersand-dom-bindings": "^3.3.3",
    "ampersand-state": "^4.2.7",
    "ampersand-version": "^1.0.0",
    "component-classes": "^1.0.0",
    "domify": "^1.0.0",
    "events-mixin": "^1.1.0",
    "get-object-path": "^0.0.2",
    "matches-selector": "^1.0.0",
    "underscore": "1.6.0"
  },
  "devDependencies": {
    "ampersand-model": "~4.0.2",
    "ampersand-rest-collection": "^2.0.3",
    "browserify": "~5.11.2",
    "jsdom": "^0.10.6",
    "phantomjs": "^1.9.7-15",
    "precommit-hook": "0.x.x",
    "run-browser": "~1.2.0",
    "tap-spec": "^0.2.0",
    "tape": "^2.14.0",
    "tape-run": "~0.3.0"
  },
  "homepage": "https://github.com/ampersandjs/ampersand-view",
  "keywords": [
    "backbone",
    "view",
    "browser",
    "browserify"
  ],
  "license": "MIT",
  "main": "./ampersand-view.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ampersandjs/ampersand-view.git"
  },
  "scripts": {
    "validate": "jshint .",
    "start": "run-browser test/index.js",
    "test": "browserify test/index.js | tape-run | tap-spec"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/9..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
